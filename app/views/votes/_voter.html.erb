<div class="container small-width">
<div class="vote-arrows pull-left">
  <div>
    <%= link_to "Up Vote", 
      post_up_vote_path(post), 
      class: "btn btn-primary" %>
  </div>
  <div>
    <strong><%= post.points %></strong>
  </div>
  <div>
    <%= link_to "Down Vote", 
      post_down_vote_path(post), 
      class: "btn btn-primary" %>
  </div>
</div>
</div>
